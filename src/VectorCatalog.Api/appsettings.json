{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "VectorCatalog": {
    "SidecarGrpcAddress": "http://sidecar:50051",
    "Redis": {
      "ConnectionString": "redis:6379",
      "DefaultCacheTtlSeconds": 300,
      "KeyPrefix": "vc:"
    },
    "Faiss": {
      "DefaultTopK": 10,
      "DefaultNprobe": 10,
      "DefaultShardKey": "nyc_taxi_2023"
    },
    "Storage": {
      "Provider": "minio",
      "MinioEndpoint": "http://minio:9000",
      "MinioAccessKey": "minioadmin",
      "MinioSecretKey": "minioadmin",
      "BucketName": "vector-catalog",
      "AdlsGen2Endpoint": "",
      "AdlsGen2AccountName": "",
      "AdlsGen2FileSystem": "vector-catalog"
    },
    "RateLimit": {
      "PermitLimit": 100,
      "WindowSeconds": 10,
      "QueueLimit": 50
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning",
        "Grpc": "Warning"
      }
    },
    "WriteTo": [
      { "Name": "Console" }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ]
  },
  "OpenTelemetry": {
    "ServiceName": "vector-catalog-api",
    "ServiceVersion": "0.1.0",
    "JaegerEndpoint": "http://jaeger:14268/api/traces"
  }
}
